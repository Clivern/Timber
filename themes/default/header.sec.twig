<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1">
{% if site_description != '' %}
    <meta name="description" content="{{ site_description|e }}" />
{% endif %}
{% if site_keywords != '' %}
    <meta name="keywords" content="{{ site_keywords|e }}" />
{% endif %}

<link rel="icon" type="image/png" href="{{ default_theme_assets_url('/img/favicon.png')|e }}">
<title>{{ site_sub_page|e }}{{ site_title|striptags|title }}</title>

{# Load Stylesheets #}
<link rel="stylesheet" type="text/css" href="{{ url_schema|e }}://fonts.googleapis.com/css?family={{ google_font|e }}:300,400,600,700">
<style type="text/css">
    html{font-family: "{{ google_font|e }}", "Helvetica Neue", Helvetica, Arial, sans-serif !important;}
    body{font-family: "{{ google_font|e }}", "Helvetica Neue", Helvetica, Arial, sans-serif !important;}
</style>
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/semantic-2.1.8/dist/semantic.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/font-awesome-4.4.0/css/font-awesome.min.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/wysibb/theme/default/wbbtheme.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/toastr/toastr.min.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/pace-1.0.2/themes/red/pace-theme-minimal.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/datetime/datetimepicker.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/datatables/css/jquery.dataTables.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/fullcalendar/fullcalendar.min.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/fullcalendar/fullcalendar.print.css')|e }}" media="print" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/alertifyjs/css/alertify.min.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/vendor/alertifyjs/css/themes/semantic.min.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/css/styles.css')|e }}" />
<link rel="stylesheet" type="text/css" href="{{ default_theme_assets_url('/skins/' ~ site_skin ~ '.css')|e }}" />

<script type="text/javascript" src="{{ default_theme_assets_url('/vendor/modernizr/modernizr.min.js')|e  }}"></script>


{# HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries #}
{# WARNING: Respond.js doesn't work if you view the page via file:// #}
<!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->

<style type="text/css">
    {% if site_custom_styles is defined %}
        {% autoescape false %}
            {{ site_custom_styles }}
        {% endautoescape %}
    {% endif %}
</style>

{% if header_scripts is defined %}
    <script type="text/javascript">
        {% autoescape false %}
            {{ header_scripts }}
        {% endautoescape %}
    </script>
{% endif %}
{% if injected_scripts is defined %}
    <script type="text/javascript">
        {% autoescape false %}
            {{ injected_scripts }}
        {% endautoescape %}
    </script>
{% endif %}

</head>
<body>
    <section id="main-wrapper" class="timber-current-skin sidebar-mini">